vipGlow.Thickness = 1  -- Thinner glow border
vipGlow.Transparency = 0.6
vipGlow.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
 
-- 3. Create Time Text Label
local timeLabel = Instance.new("TextLabel")
timeLabel.Name = "TimeLabel"
timeLabel.Parent = container
timeLabel.BackgroundTransparency = 1
timeLabel.Position = UDim2.new(0, 83, 0, 0)  -- Right of VIP label with 3px spacing
timeLabel.Size = UDim2.new(0, 90, 1, 0)
timeLabel.Font = Enum.Font.GothamSemibold
timeLabel.Text = "00:00:00"
timeLabel.TextScaled = true
timeLabel.TextSize = 9  -- Ultra small font
timeLabel.TextWrapped = true
timeLabel.TextXAlignment = Enum.TextXAlignment.Left  -- Left-aligned text
 
-- 4. Rainbow Color Cycle Logic
local Hue = 0
local Saturation = 0.8
local Value = 1
 
local function HSVToRGB(h, s, v)
local r, g, b
local i = math.floor(h * 6)
local f = h * 6 - i
local p = v * (1 - s)
local q = v * (1 - f * s)
local t = v * (1 - (1 - f) * s)
 
i = i % 6
if i == 0 then r, g, b = v, t, p
elseif i == 1 then r, g, b = q, v, p
elseif i == 2 then r, g, b = p, v, t
elseif i == 3 then r, g, b = p, q, v
elseif i == 4 then r, g, b = t, p, v
else r, g, b = v, p, q end
 
return Color3.new(r, g, b)
end
 
-- 5. VIP Text Pulse Animation
local function vipPulseAnimation()
while vipLabel and vipLabel.Parent do
local pulse = 0.5 + math.sin(tick() * 2) * 0.1
vipGlow.Transparency = pulse
vipLabel.TextTransparency = 0.2 + math.abs(math.sin(tick() * 4)) * 0.1
task.wait()
end
end
 
-- 6. Update Display
local function UpdateDisplay()
-- Update rainbow color
Hue = Hue + 0.008
if Hue > 1 then Hue = 0 end
local rainbowColor = HSVToRGB(Hue, Saturation, Value)
 
-- Update time display
local currentTime = os.date("%H:%M:%S")
timeLabel.Text = currentTime
timeLabel.TextColor3 = rainbowColor
end
 
-- 7. Start Animation and Update Loop
coroutine.wrap(vipPulseAnimation)()
game:GetService("RunService").Heartbeat:Connect(UpdateDisplay)local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

local NotificationHolder = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()

Notification:Notify(
    {Title = "Welcome to Black & White Script", Description = "Please join QQ Group: 946671668"},
    {OutlineColor = Color3.fromRGB(255, 255, 255), Time = 9, Type = "image"},
    {Image = "http://www.roblox.com/asset/?id=6023426923", ImageColor = Color3.fromRGB(150, 150, 150)}
)

local NotificationHolder = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Module.Lua"))()
local Notification = loadstring(game:HttpGet("https://raw.githubusercontent.com/BocusLuke/UI/main/STX/Client.Lua"))()

Notification:Notify(
    {Title = "Anti-AFK Enabled", Description = "The author has helped you enable Anti-AFK"},
